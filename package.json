{
  "name": "college-portal-monorepo",
  "version": "1.0.0",
  "scripts": {
    "install:all": "npm install",
    "dev": "concurrently \"npm run dev --workspace=backend\" \"npm run dev --workspace=frontend\"",
    "build": "npm run build --workspace=frontend",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "workspaces": [
    "backend",
    "frontend"
  ],
  "devDependencies": {
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2"
  }
}
